#headerBox{
	width: 100%;
	height: 172px;
	border-bottom:2px solid #679800;
	
	min-width: 900px;
}

.header_top{
	width: 100%;
	height: 34px;
	padding-top:2px ;
	box-sizing: border-box;
	background: #f7f7f7;
}
.header_top_title{
	width: 1201px;
	height: 34px;
	margin: 0 auto;
    
	box-sizing: border-box;
}
.header_top_title .address_text{
	color: #969696;
	width: 100%;
	height: 100%;
	float: left;
	overflow: hidden;
}
.header_top_address{
	width: 50px;
	height: 20px;
	border: 1px solid #dcdcdc;
    display: inline-block;
  position: relative;


}
.header_top_address:after{
	content: "";
	width: 0;
	height: 0;
	border: 5px solid transparent;
	border-top:8px solid #cccccc ;
	position: absolute;
	top: 8px;
	right: 5px;
	transition: 0.5s;
}
.header_top_mid{
	width: 230px;
	display: inline-block;
  
}
.header_top_a{
	color: #969696;
	margin-right: 7px;
	cursor: pointer;
}
.header_top_a:hover{
	text-decoration: none;
	color: #669900;
}
.header_top_nav{
	display: inline-block;
	width: 79px;
	height: 27px;
	position: relative;
	border: 1px solid transparent;
}
.header_top_nav .header_top_nav_first{
	text-align: center;
	line-height: 27px;
	
	color: #969696;
	width: 70px;
	height: 27px;
	cursor: pointer;
}

.header_top_nav .header_top_nav_first:after{
	content: "";
	width: 0;
	height: 0;
	border: 5px solid transparent;
	border-top:8px solid #cccccc ;
	position: absolute;
	top: 10px;
	right: 5px;
	transition: 0.5s;
}
.header_top_nav .header_top_nav_first:hover:after{
	transform: rotate(180deg);
	position: absolute;
	top: 5px;
	right: 4px;
}
.header_top_nav:hover{
	border: 1px solid #dadada;
	background: white;
	 
}

.header_top_nav  li:hover ul{
	display: block;
}
.header_top_nav ul{
	display: none;
	position: absolute;
	box-shadow: 1px 1px  1px  #666666,-1px 1px  1px  #666666;
	left: -1px;
	width: 79px;
	height: 111px;
	border: 1px solid #dadada;
	background: white;
}
.header_top_nav:hover{
	box-shadow: 1px -1px  1px  #666666,-1px -1px 1px  #666666;
}
.header_top_nav ul li{
	width: 100%;
	height: 22px;
	cursor: pointer;
	text-align: center;
	line-height: 22px;
	
}
.header_top_nav ul li a{
	color: #969696;
	}
.header_top_nav ul li a:hover{
	color: #669900;
	text-decoration: none;
}

.header_top_address:hover:after{
	transform: rotate(180deg);
	position: absolute;
	top: 3px;
	right: 4px;
}
.header_mid{
	width: 1201px;
	height: 105px;
	
	margin: 0 auto;
    
}
.header_top_pic{
	display: inline-block;
	margin-left:20px ;
	width: 92px;
	height: 100%;
}
.header_bottom{
	width: 1201px;
	height: 34px;
	position: relative;
