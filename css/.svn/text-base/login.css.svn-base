.bigbox{
	width: 100%;
	height: 719px;

}
.box_img{
	padding-left:194px ;
	box-sizing: border-box;
}

.inp_box{
	width: 100%;
	height: 487px;
	
}
.inp_box_mid{
	width: 928px;
	height: 100%;
	margin: 0 auto;
	
	padding-top: 25px;
	box-sizing: border-box;
}
.inp_box_mid_top{
	width: 100%;
	height: 26px;
	position: relative;
}
.inp_box_mid_top a{
	display: inline-block;
	width: 73px;
	height: 26px;
}
.registered{
	display: inline-block;
	width: 70px;
	height: 25px;
	border:1px solid #34b742 ;
	background: #fff4f0;
	color: #515151;
	text-align: center;
	line-height: 25px;
	position: absolute;
	right: 0;
}
.registered:hover{
	color: #515151;
}
.inp_box_mid_mid{
   margin-top:10px ;
	width: 100%;
	height: 438px;
	
	
}
.inp_box_mid_mid img{
	float: left;
}
.inp_box_mid_mid .login_box {
	float: right;
	width: 400px;
	height: 396px;
	border: 1px solid #cdcdcd;
}
.login_box ul li{
	float:left ;
}
.login_box li a{
	
	float: left;
	width: 198px;
	height: 30px;
	background: white;
	border-left:1px solid #cdcdcd;
		border-bottom:1px solid #cdcdcd;
		font-size: 16px;
		padding-top: 5px;
		padding-left: 50px;
		box-sizing: border-box;
}
.login_box li a:hover{
	color: black;
	
}
.login_box li:first-child a{
	border-bottom:transparent;
	border-top:2px solid #69af05 ;
}
.login_toggle{
	width: 100%;
	height: 310px;
	padding-left: 36px;
	float: left;
	box-sizing: border-box;
}
.login_toggle span{
	color: #515151;
}
input[type="text"]{
	border: 1px solid #cdcdcd;
	width: 324px;
	height: 37px;
	text-indent: 10px;
	margin-top: 7px;
	margin-bottom:20px ;
}
input[type="text"].inp_code{
	float: left;
	width: 106px;
	height: 37px;
	
}
.code{
	float: left;
	background: url(../img/vcode.png) no-repeat;
	background-size:106px 37px ;
	width: 106px;
	height: 37px;
	margin-top: 7px;
	margin-left: 10px;
	border: 1px solid black;
}
.youxuan{
	width: 100%;
	height: 310px;
	display: none;
	padding-top: 20px;
	box-sizing: border-box;
   position: relative;
}
.check_pass{
	position: absolute;
	right: 30px;
	bottom: 60px;
}
.check_pass:hover{
	color: black;
	text-decoration: underline;
}
input[type="button"]{
	width: 328px;
	height: 38px;
	background: #6e9b0c;
	border: none;
	border-radius:5px ;
	cursor: pointer;
	
	
	color: white;
}



.suyun{
	width: 100%;
	height: 340px;
	display: none;
	padding-top: 20px;
	box-sizing: border-box;
	 position: relative;
}
.pro:hover{
	color: black;
	text-decoration: underline;
}
.login_img_first{
	position: absolute;
	right: 40px;
	top: 50px;
}
.login_img_second{
	position: absolute;
	right: 40px;
	top: 135px;
}
.login_bottom{
	width: 100%;
	height: 80px;
	float: left;
	border-top:1px solid #cdcdcd ;
	padding-top: 8px;
	box-sizing: border-box;
}
.inp_box_mid_bottom{
	margin-top: 20px;
	width: 100%;
	height: 80px;
	float: left;
	
	
}
.login_bottom div{
	width: 80px;
	height: 40px;
	float: left;
	border-right:1px solid #cdcdcd  ;
	padding-left: 20px;
	box-sizing: border-box; 
	position: relative;
}
.login_bottom div a{
	position: absolute;
}
.login_bottom div a:hover{
	text-decoration: underline;

}
.inp_box_mid_bottom ul{
	width: 330px;
	
	margin:0 auto ;
}
.inp_box_mid_bottom ul li{
	float: left;
	width:60px;
	margin-right:5px;
	height: 20px;
	border-right:1px solid #926291 ;
	text-align: center;
}
.inp_box_mid_bottom ul li:last-child{
	border: transparent;
}
.inp_box_mid_bottom a:hover{
	color: #666666;
	text-decoration: underline;
}
